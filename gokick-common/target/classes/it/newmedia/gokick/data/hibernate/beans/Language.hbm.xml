<?xml version="1.0" encoding="ISO-8859-1"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
  <class mutable="false" table="LANGUAGES" name="it.newmedia.gokick.data.hibernate.beans.Language">
    <id unsaved-value="null" name="id" column="ID_LANGUAGE">
      <generator class="native"/>
    </id>
    <property name="language" column="LANGUAGE"/>
    <property name="label" column="LABEL"/>
    <property name="localeStr" column="LOCALE"/>
    <property name="position" column="POSITION"/>
    <property name="enabled" column="ENABLED"/>
  </class>
</hibernate-mapping>
