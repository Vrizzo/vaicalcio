<?xml version="1.0" encoding="ISO-8859-1"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
  <class mutable="false" table="COUNTRIES" name="it.newmedia.gokick.data.hibernate.beans.Country">
    <cache usage="read-only"/>
    <id unsaved-value="null" name="id" column="ID_COUNTRY">
      <generator class="native"/>
    </id>
    <property name="name" column="NAME"/>
    <property name="position" column="POSITION"/>
    <property name="totUsers" column="TOT_USERS"/>
    <set inverse="false" lazy="true" name="sportCenterList">
      <key column="ID_COUNTRY"/>
      <one-to-many class="it.newmedia.gokick.data.hibernate.beans.SportCenter"/>
    </set>
    <set inverse="false" lazy="true" name="provinceList">
      <key column="ID_COUNTRY"/>
      <one-to-many class="it.newmedia.gokick.data.hibernate.beans.Province"/>
    </set>
    <set inverse="false" lazy="true" name="userList">
      <key column="ID_COUNTRY"/>
      <one-to-many class="it.newmedia.gokick.data.hibernate.beans.User"/>
    </set>
  </class>
</hibernate-mapping>
