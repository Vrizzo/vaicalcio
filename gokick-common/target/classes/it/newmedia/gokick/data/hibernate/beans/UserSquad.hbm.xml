<?xml version="1.0" encoding="ISO-8859-1"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
  <class table="USERS_SQUADS" name="it.newmedia.gokick.data.hibernate.beans.UserSquad">
    <id unsaved-value="null" name="id" column="ID_USER_SQUAD">
      <generator class="native"/>
    </id>
    <many-to-one column="ID_USER" cascade="none" class="it.newmedia.gokick.data.hibernate.beans.User" lazy="false" not-null="false" name="user"/>
    <many-to-one column="ID_SQUAD" cascade="none" class="it.newmedia.gokick.data.hibernate.beans.Squad" lazy="false" not-null="false" name="squad"/>
    <property name="userSquadStatus" column="USER_SQUAD_STATUS"/>
    <property name="owner" column="OWNER"/>
  </class>
</hibernate-mapping>
