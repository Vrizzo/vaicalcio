<?xml version="1.0" encoding="ISO-8859-1"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
  <class table="USERS" name="it.newmedia.gokick.data.hibernate.beans.User">
    <id unsaved-value="null" name="id" column="ID_USER">
      <generator class="native"/>
    </id>
    <many-to-one column="ID_COUNTRY" cascade="none" class="it.newmedia.gokick.data.hibernate.beans.Country" lazy="false" not-null="true" name="country"/>
    <many-to-one column="ID_PROVINCE" cascade="none" class="it.newmedia.gokick.data.hibernate.beans.Province" lazy="false" not-null="false" name="province"/>
    <many-to-one column="ID_CITY" cascade="none" class="it.newmedia.gokick.data.hibernate.beans.City" lazy="false" not-null="false" name="city"/>
    <many-to-one column="ID_BIRTHDAY_COUNTRY" cascade="none" class="it.newmedia.gokick.data.hibernate.beans.Country" lazy="false" not-null="false" name="birthdayCountry"/>
    <many-to-one column="ID_BIRTHDAY_PROVINCE" cascade="none" class="it.newmedia.gokick.data.hibernate.beans.Province" lazy="false" not-null="false" name="birthdayProvince"/>
    <many-to-one column="ID_BIRTHDAY_CITY" cascade="none" class="it.newmedia.gokick.data.hibernate.beans.City" lazy="false" not-null="false" name="birthdayCity"/>
    <many-to-one column="ID_NATIONALITY_COUNTRY" cascade="none" class="it.newmedia.gokick.data.hibernate.beans.Country" lazy="false" not-null="false" name="nationalityCountry"/>
    <many-to-one column="ID_PLAYER_ROLE" cascade="none" class="it.newmedia.gokick.data.hibernate.beans.PlayerRole" lazy="proxy" not-null="false" name="playerRole"/>
    <many-to-one column="ID_FAVOURITE_FOOTBALL_TEAM" cascade="none" class="it.newmedia.gokick.data.hibernate.beans.FootballTeam" lazy="no-proxy" not-null="false" name="footballTeam"/>
    <many-to-one column="ID_PLAYER_FOOT" cascade="none" class="it.newmedia.gokick.data.hibernate.beans.PlayerFoot" lazy="proxy" not-null="false" name="playerFoot"/>
    <property name="email" column="EMAIL"/>
    <property name="physicalConditionDetail" column="PHYSICAL_CONDITION_DETAIL"/>
    <property name="password" column="PASSWORD"/>
    <property name="firstName" column="FIRST_NAME"/>
    <property name="lastName" column="LAST_NAME"/>
    <property name="birthDay" column="BIRTHDAY"/>
    <property name="address" column="ADDRESS"/>
    <property name="cap" column="CAP"/>
    <property name="mobile" column="MOBILE"/>
    <property name="telephone" column="TELEPHONE"/>
    <property name="sex" column="SEX"/>
    <property name="playerHeight" column="PLAYER_HEIGHT"/>
    <property name="playerWeight" column="PLAYER_WEIGHT"/>
    <property name="playerTitle" column="PLAYER_TITLE"/>
    <property name="playerMainFeature" column="PLAYER_MAIN_FEATURE"/>
    <property name="playerShirtNumber" column="PLAYER_SHIRT_NUMBER"/>
    <property name="infoFavouritePlayer" column="INFO_FAVOURITE_PLAYER"/>
    <property name="infoHobby" column="INFO_HOBBY"/>
    <property name="infoDream" column="INFO_DREAM"/>
    <property name="infoAnnounce" column="INFO_ANNOUNCE"/>
    <property name="marketEnabled" column="MARKET_ENABLED"/>
    <property name="generalTermsAccepted" column="GENERAL_TERMS_ACCEPTED"/>
    <property name="privacyAccepted" column="PRIVACY_ACCEPTED"/>
    <property name="newsletterEnabled" column="NEWSLETTER_ENABLED"/>
    <property name="organizeEnabled" column="ORGANIZE_ENABLED"/>
    <property name="lastIP" column="LAST_IP"/>
    <property name="lastLogin" column="LAST_LOGIN"/>
    <property name="prevLogin" column="PREV_LOGIN"/>
    <property name="lastVisitMessages" column="LAST_VISIT_MESSAGES"/>
    <property name="prevVisitMessages" column="PREV_VISIT_MESSAGES"/>
    <property name="userStatus" column="USER_STATUS"/>
    <property name="forumNickname" column="FORUM_NICKNAME"/>
    <property name="alertOnMessages" column="ALERT_ON_MESSAGES"/>
    <property name="alertOnSquadRequest" column="ALERT_ON_SQUAD_REQUEST"/>
    <property name="alertOnSquadInsert" column="ALERT_ON_SQUAD_INSERT"/>
    <property name="alertOnRegistrationStart" column="ALERT_ON_REGISTRATION_START"/>
    <property name="alertOnChange" column="ALERT_ON_CHANGES"/>
    <property name="alertOnSquadOutAccepted" column="ALERT_ON_SQUAD_OUT_ACCEPTED"/>
    <property name="alertOnReportCreated" column="ALERT_ON_REPORT_CREATED"/>
    <property name="anonymousEnabled" column="ANONYMOUS_ENABLED"/>
    <property name="recordedMatches" column="RECORDED_MATCHES"/>
    <property name="recordedChallenges" column="RECORDED_CHALLENGES"/>
    <property name="playedMatches" column="PLAYED_MATCHES"/>
    <property name="playedChallenges" column="PLAYED_CHALLENGES"/>
    <property name="checkPending" column="CHECK_PENDING"/>
    <property name="checkPassword" column="CHECK_PASSWORD"/>
    <property name="checkPasswordExpire" column="CHECK_PASSWORD_EXPIRE"/>
    <set inverse="true" cascade="all-delete-orphan" lazy="true" name="squads">
      <key column="ID_USER"/>
      <one-to-many class="it.newmedia.gokick.data.hibernate.beans.Squad"/>
    </set>
    <set inverse="true" lazy="true" name="pictureCards">
      <key column="ID_USER"/>
      <one-to-many class="it.newmedia.gokick.data.hibernate.beans.PictureCard"/>
    </set>
    <set inverse="true" lazy="true" name="userPermissions">
      <key column="ID_USER"/>
      <one-to-many class="it.newmedia.gokick.data.hibernate.beans.UsersPermission"/>
    </set>
    <many-to-one column="ID_PHYSICAL_CONDITION" cascade="none" class="it.newmedia.gokick.data.hibernate.beans.PhysicalCondition" lazy="proxy" not-null="false" name="physicalCondition"/>
    <set inverse="true" cascade="all-delete-orphan" lazy="true" name="userInvitations">
      <key column="ID_USER_OWNER"/>
      <one-to-many class="it.newmedia.gokick.data.hibernate.beans.UserInvitation"/>
    </set>
    <property name="maxInvitations" column="MAX_INVITATIONS"/>
    <property name="invited_by" column="INVITED_BY"/>
    <property name="thirdOrganizedMatch" column="THIRD_ORGANIZED_MATCH"/>
    <property name="facebookIdUser" column="FACEBOOK_ID_USER"/>
    <property name="facebookAccessToken" column="FACEBOOK_ACCESS_TOKEN"/>
    <property name="facebookPostOnMatchCreation" column="FACEBOOK_POST_ON_MATCH_CREATION"/>
    <property name="facebookPostOnMatchRegistration" column="FACEBOOK_POST_ON_MATCH_REGISTRATION"/>
    <property name="facebookPostOnMatchRecorded" column="FACEBOOK_POST_ON_MATCH_RECORDED"/>
    <property name="cobrandCode" column="COBRAND_CODE"/>
    <property name="created" column="CREATED"/>
  </class>
</hibernate-mapping>
