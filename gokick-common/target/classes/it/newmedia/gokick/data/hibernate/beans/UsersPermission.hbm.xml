<?xml version="1.0" encoding="ISO-8859-1"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
  <class table="USERS_PERMISSIONS" name="it.newmedia.gokick.data.hibernate.beans.UsersPermission">
    <id unsaved-value="null" name="id" column="ID_USER_PERMISSION">
      <generator class="native"/>
    </id>
    <many-to-one column="ID_USER" cascade="none" class="it.newmedia.gokick.data.hibernate.beans.User" lazy="false" not-null="false" name="user"/>
    <property name="permission" column="PERMISSION"/>
    <property name="idCountry" column="ID_COUNTRY"/>
    <property name="idProvince" column="ID_PROVINCE"/>
    <property name="idCity" column="ID_CITY"/>
    <property name="fromDate" column="FROM_DATE"/>
  </class>
</hibernate-mapping>
