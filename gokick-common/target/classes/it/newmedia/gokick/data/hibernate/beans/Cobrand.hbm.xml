<?xml version="1.0" encoding="ISO-8859-1"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
  <class mutable="false" table="COBRANDS" name="it.newmedia.gokick.data.hibernate.beans.Cobrand">
    <cache usage="read-only"/>
    <id unsaved-value="null" name="id" column="ID_COBRAND">
      <generator class="native"/>
    </id>
    <property name="code" column="CODE"/>
    <property name="domain" column="DOMAIN"/>
    <property name="facebookAppCode" column="FACEBOOK_APP_CODE"/>
    <property name="facebookAppId" column="FACEBOOK_APP_ID"/>
    <property name="gatewayUrl" column="GATEWAY_URL"/>
    <property name="googleAnalyticsKey" column="GOOGLE_ANALYTICS_KEY"/>
    <property name="homePageUrl" column="HOME_PAGE_URL"/>
    <property name="siteUrl" column="SITE_URL"/>
    <property name="type" column="TYPE"/>
  </class>
</hibernate-mapping>
