<?xml version="1.0" encoding="ISO-8859-1"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
  <class table="USERS_MESSAGES" name="it.newmedia.gokick.data.hibernate.beans.UsersMessage">
    <id unsaved-value="null" name="id" column="ID_USER_MESSAGE">
      <generator class="native"/>
    </id>
    <property name="idUser" column="ID_USER"/>
    <property name="idMessage" column="ID_MESSAGE"/>
    <property name="seen" column="SEEN"/>
  </class>
</hibernate-mapping>
