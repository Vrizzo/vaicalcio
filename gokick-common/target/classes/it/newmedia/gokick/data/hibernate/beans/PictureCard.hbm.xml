<?xml version="1.0" encoding="ISO-8859-1"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
  <class table="PICTURE_CARDS" name="it.newmedia.gokick.data.hibernate.beans.PictureCard">
    <id unsaved-value="null" name="id" column="ID_PICTURE_CARD">
      <generator class="native"/>
    </id>
    <many-to-one column="ID_USER" cascade="none" class="it.newmedia.gokick.data.hibernate.beans.User" lazy="false" not-null="false" name="user"/>
    <property name="filename" column="FILENAME"/>
    <property name="created" column="CREATED"/>
    <property name="pictureCardStatus" column="PICTURE_CARD_STATUS"/>
  </class>
</hibernate-mapping>
