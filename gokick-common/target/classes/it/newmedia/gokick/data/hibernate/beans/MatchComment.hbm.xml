<?xml version="1.0" encoding="ISO-8859-1"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
  <class table="MATCH_COMMENTS" name="it.newmedia.gokick.data.hibernate.beans.MatchComment">
    <id unsaved-value="null" name="id" column="ID_MATCH_COMMENT">
      <generator class="native"/>
    </id>
    <many-to-one column="ID_MATCH" cascade="none" class="it.newmedia.gokick.data.hibernate.beans.Match" lazy="false" not-null="true" name="match"/>
    <property name="textDetail" column="TEXT_DETAIL"/>
    <many-to-one column="ID_USER" cascade="none" class="it.newmedia.gokick.data.hibernate.beans.User" lazy="false" not-null="true" name="user"/>
    <property name="approved" column="APPROVED"/>
    <property name="deleted" column="DELETED"/>
    <property name="created" column="CREATED"/>
  </class>
</hibernate-mapping>
