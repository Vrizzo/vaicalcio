<?xml version="1.0" encoding="ISO-8859-1"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
  <class table="V_PLAYER_MATCH_CHALLENGE_ARCHIVED" name="it.newmedia.gokick.data.hibernate.beans.VPlayerMatchChallangeArchived">
    <id unsaved-value="null" name="idPlayer" column="ID_PLAYER">
      <generator class="native"/>
    </id>
    <property name="isChallange" column="IS_CHALLANGE"/>
    <property name="isMatch" column="IS_MATCH"/>
    <property name="gameGoalsOne" column="GAME_GOALS_ONE"/>
    <property name="gameGoalsTwo" column="GAME_GOALS_TWO"/>
    <property name="gameStart" column="GAME_START"/>
    <property name="goals" column="GOALS"/>
    <property name="idGame" column="ID_GAME"/>
    <property name="idUser" column="ID_USER"/>
    <property name="idUserOwner" column="ID_USER_OWNER"/>
    <property name="ownGoals" column="OWN_GOALS"/>
    <property name="playerStatus" column="PLAYER_STATUS"/>
    <property name="playerType" column="PLAYER_TYPE"/>
    <property name="vote" column="VOTE"/>
  </class>
</hibernate-mapping>
