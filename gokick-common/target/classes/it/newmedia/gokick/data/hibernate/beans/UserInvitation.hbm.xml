<?xml version="1.0" encoding="ISO-8859-1"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
  <class table="USER_INVITATIONS" name="it.newmedia.gokick.data.hibernate.beans.UserInvitation">
    <id unsaved-value="null" name="id" column="ID_USER_INVITATION">
      <generator class="native"/>
    </id>
    <many-to-one column="ID_USER_OWNER" cascade="none" class="it.newmedia.gokick.data.hibernate.beans.User" fetch="join" lazy="no-proxy" not-null="true" name="userOwner"/>
    <many-to-one column="ID_USER" cascade="none" class="it.newmedia.gokick.data.hibernate.beans.User" fetch="join" lazy="no-proxy" not-null="false" name="user"/>
    <property name="code" column="CODE"/>
    <property name="created" column="CREATED"/>
  </class>
</hibernate-mapping>
