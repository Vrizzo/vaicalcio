<?xml version="1.0" encoding="ISO-8859-1"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
  <class table="FOOTBALL_TEAMS" name="it.newmedia.gokick.data.hibernate.beans.FootballTeam">
    <id unsaved-value="null" name="id" column="ID_FOOTBALL_TEAM">
      <generator class="native"/>
    </id>
    <many-to-one column="ID_COUNTRY" cascade="none" class="it.newmedia.gokick.data.hibernate.beans.Country" lazy="false" not-null="false" name="country"/>
    <many-to-one column="ID_COUNTRY_REGION" cascade="none" class="it.newmedia.gokick.data.hibernate.beans.CountryRegion" lazy="false" not-null="false" name="countryRegion"/>
    <property name="code" column="CODE"/>
    <property name="name" column="NAME"/>
  </class>
</hibernate-mapping>
