<?xml version="1.0" encoding="ISO-8859-1"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
  <class table="SQUADS" name="it.newmedia.gokick.data.hibernate.beans.Squad">
    <id unsaved-value="null" name="id" column="ID_SQUAD">
      <generator class="native"/>
    </id>
    <many-to-one column="ID_USER" cascade="none" class="it.newmedia.gokick.data.hibernate.beans.User" lazy="false" not-null="false" name="user"/>
    <property name="name" column="NAME"/>
    <property name="description" column="DESCRIPTION"/>
    <property name="marketEnabled" column="MARKET_ENABLED"/>
    <property name="webSite" column="WEB_SITE"/>
    <property name="photoSite" column="PHOTO_SITE"/>
    <property name="videoSite" column="VIDEO_SITE"/>
    <property name="playingWeekdays" column="PLAYING_WEEKDAYS"/>
    <property name="position" column="POSITION"/>
    <property name="hiddenEnabled" column="HIDDEN_ENABLED"/>
    <property name="created" column="CREATED"/>
    <set inverse="true" cascade="all-delete-orphan" lazy="true" name="userSquadList">
      <key column="ID_SQUAD"/>
      <one-to-many class="it.newmedia.gokick.data.hibernate.beans.UserSquad"/>
    </set>
  </class>
</hibernate-mapping>
