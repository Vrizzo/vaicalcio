<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE urlrewrite PUBLIC "-//tuckey.org//DTD UrlRewrite 2.6//EN"
        "http://tuckey.org/res/dtds/urlrewrite2.6.dtd">

<!--
    Configuration file for UrlRewriteFilter
    http://tuckey.org/urlrewrite/
-->
<urlrewrite>
    <!-- /home -->
    <rule>
        <note>
            Home page
        </note>
        <from>^/$</from>
        <to type="redirect">home.action</to>
    </rule>
    <!-- return dinamically array for messages as javascript file -->
    <rule>
        <from>/js/messagesjsfromjsp.js</from>
        <to type="forward">/jsp/messagesjsfromjsp.jsp</to>
    </rule>
</urlrewrite>

