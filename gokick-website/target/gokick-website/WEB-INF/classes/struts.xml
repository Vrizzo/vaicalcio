        <param name="info">${errorCode}</param>
      </result>
    </action>

    <!-- LOGOUT -->
    <action name="logout" class="it.newmedia.gokick.site.web.actions.user.UserLogoutAction">
      <result type="redirectAction">home</result>
    </action>

